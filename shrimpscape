meta {
name = "Shrimpscape";
    description = "Highlights consumables, e.g. for PvM";
}

//Colors
#define SHRIMP "ffa87215"
#define STRPOT "fff4f281"
#define PPOT "ff44d19a"


#define FOOD(_name ) if (name:_name && noted:false) { \
    color = SHRIMP; \
}

#define POTION(_name ) if (name:_name && noted:false) { \
    color = STRPOT; \
}

#define PRAY(_name ) if (name:_name && noted:false) { \
    color = PPOT; \
}

#define PRAYBOOST(_name ) if (name:_name && noted:false) { \
    color = PPOT; \
}

#define FOODBOOST(_name ) if (name:_name && noted:false) { \
    color = SHRIMP; \
}




